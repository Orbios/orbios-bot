{
  "name": "@team-fusion/bot",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "description": "Team Fusion Discord Bot",
  "main": "dist/app.js",
  "engines": {
    "node": ">=18.x"
  },
  "scripts": {
    "dev": "cross-env-shell ENV_FILE=.env.dev nodemon --exec tsx src/app.ts",
    "deploy-commands": "nodemon --exec tsx src/deployCommands.ts",
    "build": "npx tsc",
    "prestart": "npm run build",
    "start": "node dist/app.js",
    "format": "prettier --write .",
    "lint": "eslint src --ext .ts --fix",
    "ci:lint": "eslint src --ext .ts --quiet"
  },
  "author": "Orbios",
  "license": "MIT",
  "dependencies": {
    "axios": "1.7.9",
    "discord.js": "14.17.3",
    "dotenv": "16.4.7",
    "franc-min": "6.2.0",
    "openai": "4.77.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "eslint": "^9.36.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-unused-imports": "^4.2.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "ts-node": "10.9.2",
    "tsx": "^4.20.6",
    "typescript": "5.7.3",
    "typescript-eslint": "^8.44.0"
  }
}
